{
  "name": "adapt-carousel",
  "version": "0.0.1",
  "description": "Adaptable carousel that works well on ssr",
  "private": true,
  "devDependencies": {
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "typescript": "3.6.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "classnames": "2.2.6",
    "@types/classnames": "2.2.9"
  },
